<% include head %>

<% for(var i = 0; i < posts.length; i++) {%>
    <div>    
        <h2><%= posts[i].title %></h2>
        <p><strong>Author: </strong><%= posts[i].author %></p>
        <p><strong>Date: </strong><%= posts[i].pubDate %></p>
        <p><%= posts[i].content %></p>
    </div>

<% } %> 


